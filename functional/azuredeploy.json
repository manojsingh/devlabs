{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "vnet_name": {
            "defaultValue": "test-vnet",
            "type": "String"
        },
        "vnet_addressPrefixes": {
            "defaultValue": "10.4.0.0/16",
            "type": "String"
        },
        "front_subnet_name": {
            "defaultValue": "FE",
            "type": "String"
        },
        "fesubnet_addressPrefixes": {
            "defaultValue": "10.4.0.0/24",
            "type": "String"
        },
        "backend_subnet_name": {
            "defaultValue": "BE",
            "type": "String"
        },
        "besubnet_addressPrefixes": {
            "defaultValue": "10.4.1.0/24",
            "type": "String"
        },
        "dbsubnet_name": {
            "defaultValue": "DB",
            "type": "String"
        },
        "dbsubnet_addressPrefixes": {
            "defaultValue": "10.4.2.0/24",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Network/virtualNetworks",
            "apiVersion": "2020-03-01",
            "name": "[parameters('vnet_name')]",
            "location": "eastus",
            "properties": {
                "addressSpace": {
                    "addressPrefixes": [
                        "[parameters('vnet_addressPrefixes')]"
                    ]
                },
                "subnets": [
                    {
                        "name": "[parameters('front_subnet_name')]",
                        "properties": {
                            "addressPrefix": "[parameters('fesubnet_addressPrefixes')]",
                            "serviceEndpoints": [
                                {
                                    "service": "Microsoft.Web",
                                    "locations": [
                                        "*"
                                    ]
                                },
                                {
                                    "service": "Microsoft.Sql",
                                    "locations": [
                                        "eastus"
                                    ]
                                },
                                {
                                    "service": "Microsoft.AzureActiveDirectory",
                                    "locations": [
                                        "*"
                                    ]
                                },
                                {
                                    "service": "Microsoft.KeyVault",
                                    "locations": [
                                        "*"
                                    ]
                                }
                            ],
                            "delegations": [],
                            "privateEndpointNetworkPolicies": "Enabled",
                            "privateLinkServiceNetworkPolicies": "Enabled"
                        }
                    },
                    {
                        "name": "[parameters('backend_subnet_name')]",
                        "properties": {
                            "addressPrefix": "[parameters('besubnet_addressPrefixes')]",
                            "serviceEndpoints": [
                                {
                                    "service": "Microsoft.AzureActiveDirectory",
                                    "locations": [
                                        "*"
                                    ]
                                },
                                {
                                    "service": "Microsoft.KeyVault",
                                    "locations": [
                                        "*"
                                    ]
                                },
                                {
                                    "service": "Microsoft.Sql",
                                    "locations": [
                                        "eastus"
                                    ]
                                },
                                {
                                    "service": "Microsoft.Storage",
                                    "locations": [
                                        "eastus",
                                        "westus"
                                    ]
                                },
                                {
                                    "service": "Microsoft.Web",
                                    "locations": [
                                        "*"
                                    ]
                                }
                            ],
                            "delegations": [],
                            "privateEndpointNetworkPolicies": "Enabled",
                            "privateLinkServiceNetworkPolicies": "Enabled"
                        }
                    },
                    {
                        "name": "[parameters('db_subnet_name')]",
                        "properties": {
                            "addressPrefix": "[parameters('dbsubnet_addressPrefixes')]",
                            "serviceEndpoints": [
                                {
                                    "service": "Microsoft.AzureActiveDirectory",
                                    "locations": [
                                        "*"
                                    ]
                                },
                                {
                                    "service": "Microsoft.AzureCosmosDB",
                                    "locations": [
                                        "*"
                                    ]
                                },
                                {
                                    "service": "Microsoft.KeyVault",
                                    "locations": [
                                        "*"
                                    ]
                                },
                                {
                                    "service": "Microsoft.ServiceBus",
                                    "locations": [
                                        "*"
                                    ]
                                },
                                {
                                    "service": "Microsoft.Sql",
                                    "locations": [
                                        "eastus"
                                    ]
                                },
                                {
                                    "service": "Microsoft.Storage",
                                    "locations": [
                                        "eastus",
                                        "westus"
                                    ]
                                },
                                {
                                    "service": "Microsoft.Web",
                                    "locations": [
                                        "*"
                                    ]
                                }
                            ],
                            "delegations": [],
                            "privateEndpointNetworkPolicies": "Enabled",
                            "privateLinkServiceNetworkPolicies": "Enabled"
                        }
                    }
                ],
                "virtualNetworkPeerings": [],
                "enableDdosProtection": false,
                "enableVmProtection": false
            }
        },
        {
            "type": "Microsoft.Network/virtualNetworks/subnets",
            "apiVersion": "2020-03-01",
            "name": "[concat(parameters('vnet_name'), '/BE')]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/virtualNetworks', parameters('vnet_name'))]"
            ],
            "properties": {
                "addressPrefix": "10.4.1.0/24",
                "serviceEndpoints": [
                    {
                        "service": "Microsoft.AzureActiveDirectory",
                        "locations": [
                            "*"
                        ]
                    },
                    {
                        "service": "Microsoft.KeyVault",
                        "locations": [
                            "*"
                        ]
                    },
                    {
                        "service": "Microsoft.Sql",
                        "locations": [
                            "eastus"
                        ]
                    },
                    {
                        "service": "Microsoft.Storage",
                        "locations": [
                            "eastus",
                            "westus"
                        ]
                    },
                    {
                        "service": "Microsoft.Web",
                        "locations": [
                            "*"
                        ]
                    }
                ],
                "delegations": [],
                "privateEndpointNetworkPolicies": "Enabled",
                "privateLinkServiceNetworkPolicies": "Enabled"
            }
        },
        {
            "type": "Microsoft.Network/virtualNetworks/subnets",
            "apiVersion": "2020-03-01",
            "name": "[concat(parameters('vnet_name'), '/DB')]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/virtualNetworks', parameters('vnet_name'))]"
            ],
            "properties": {
                "addressPrefix": "10.4.2.0/24",
                "serviceEndpoints": [
                    {
                        "service": "Microsoft.AzureActiveDirectory",
                        "locations": [
                            "*"
                        ]
                    },
                    {
                        "service": "Microsoft.AzureCosmosDB",
                        "locations": [
                            "*"
                        ]
                    },
                    {
                        "service": "Microsoft.KeyVault",
                        "locations": [
                            "*"
                        ]
                    },
                    {
                        "service": "Microsoft.ServiceBus",
                        "locations": [
                            "*"
                        ]
                    },
                    {
                        "service": "Microsoft.Sql",
                        "locations": [
                            "eastus"
                        ]
                    },
                    {
                        "service": "Microsoft.Storage",
                        "locations": [
                            "eastus",
                            "westus"
                        ]
                    },
                    {
                        "service": "Microsoft.Web",
                        "locations": [
                            "*"
                        ]
                    }
                ],
                "delegations": [],
                "privateEndpointNetworkPolicies": "Enabled",
                "privateLinkServiceNetworkPolicies": "Enabled"
            }
        },
        {
            "type": "Microsoft.Network/virtualNetworks/subnets",
            "apiVersion": "2020-03-01",
            "name": "[concat(parameters('vnet_name'), '/FE')]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/virtualNetworks', parameters('vnet_name'))]"
            ],
            "properties": {
                "addressPrefix": "10.4.0.0/24",
                "serviceEndpoints": [
                    {
                        "service": "Microsoft.Web",
                        "locations": [
                            "*"
                        ]
                    },
                    {
                        "service": "Microsoft.Sql",
                        "locations": [
                            "eastus"
                        ]
                    },
                    {
                        "service": "Microsoft.AzureActiveDirectory",
                        "locations": [
                            "*"
                        ]
                    },
                    {
                        "service": "Microsoft.KeyVault",
                        "locations": [
                            "*"
                        ]
                    }
                ],
                "delegations": [],
                "privateEndpointNetworkPolicies": "Enabled",
                "privateLinkServiceNetworkPolicies": "Enabled"
            }
        }
    ]
}